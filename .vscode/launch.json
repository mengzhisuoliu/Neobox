  "version": "0.2.0",
  "configurations": [

    {
      "name": "(Windows) 启动 Neobox",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\install\\bin\\neobox.exe",
      "args": [
        // "${workspaceFolder}\\install\\bin\\test_timer.exe"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}\\build\\Debug\\pluginmgr;${workspaceFolder}\\build\\Debug\\pluginmgr\\yjson;${workspaceFolder}\\install\\dbg;${workspaceFolder}\\..\\neobox-plugins\\install\\lugins;C:\\Qt\\6.8.2\\msvc2022_64\\bin;${env:PATH}"
        }
      ],
      "console": "externalTerminal"
    },
    {
      "name": "Windows 启动 Test",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build\\Debug\\example\\netbian.exe",
      "args": [
        "C:\\Users\\Administrator\\Desktop\\test\\test.txt"
      ],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}\\build\\Debug\\pluginmgr;${workspaceFolder}\\build\\Debug\\plugins;${env:PATH}"
        }
      ],
      "console": "externalTerminal"
    },
    {
      "name": "Linux (gdb) 启动 Neobox",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/neobox",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}/build/Debug/pluginmgr:${workspaceFolder}/build/Debug/plugins:${env:PATH}"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "为 gdb 启用整齐打印",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "将反汇编风格设置为 Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    }

  ]
}