{
  "configurations": {
    "Launch": {
      "adapter": "CodeLLDB",
      "filetypes": [ "cpp", "c", "objc", "rust" ],
      "configuration": {
        "name": "CodeLLDB",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/build/Debug/Neobox.exe",
        "args": [ ],
        "cwd": "${workspaceFolder}",
        "externalConsole": true,
        "env": {
          "PATH": "${PATH};${workspaceFolder}/install/bin;${workspaceFolder}/build/Debug/pluginmgr;${workspaceFolder}/build/Debug/plugins;${workspaceFolder}/build/Debug/yjson"
        },
        "MIMode": "lldb"
      }
    }
  }
}
